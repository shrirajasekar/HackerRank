package MasterPackage;

import java.util.Arrays;

public class amazon {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr =  {1,60,25,10,35};
		int[] out = {};
		Arrays.sort(arr);
		int fd = 90;
		int n = arr.length;
//		for( x = 0 ; x < n ; x ++ ){}
		int sum=0;
		for(int x=0;x<n;x++){
			for(int y=0;y<=x;y++){
				sum = arr[x]+arr[y];
				if(sum+30 == fd){
					System.out.println(x+"  -  " + y);
					System.out.println(arr[x]+"--- "+arr[y]);
				}
			}
		}
		for (int i:arr){
			System.out.println(i);
		}
		
	}

}
